{
  "name": "@greenmice/ngx-cesium",
  "version": "16.2.0",
  "license": "MIT",
  "description": "Angular library for working with Cesium.",
  "repository": "https://github.com/greenmice-technologies/angular-cesium",
  "keywords": [
    "Angular",
    "Cesium",
    "ngx-cesium",
    "Angular2 Cesium"
  ],
  "peerDependencies": {
    "@angular/common": "^16.0.0",
    "@angular/core": "^16.0.0",
    "cesium": ">= 1.48.0"
  },
  "dependencies": {
    "ngx-cesium-parser": "^17.1.2",
    "geodesy": "~1.1.3",
    "@types/geodesy": "~1.1.3",
    "json-string-mapper": "^1.0.0",
    "primitive-primitives": "0.0.6",
    "tslib": "^2.6.1"
  },
  "optionalDependencies": {
    "heatmap.js": "^2.0.5"
  },
  "scripts": {
    "build": "../../node_modules/.bin/tsc -p tsconfig.schematics.json",
    "copy:files": "cp -a ./schematics/ng-add ../../dist/angular-cesium/schematics",
    "copy:collection": "cp ./schematics/collection.json ../../dist/angular-cesium/schematics/collection.json",
    "postbuild": "npm run copy:files && npm run copy:collection"
  },
  "schematics": "./schematics/collection.json"
}